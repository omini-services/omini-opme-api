{
    "Auth0": {
        "Authority": "https://dev-amo5k5tptruva3yj.us.auth0.com",
        "Audience": "https://omini-opme-api-dev.endpoint",
        "Scopes": [
            "openid",
            "api:full"
        ],
        "ClientId": "WJMSo5SujECazknxLbXXI3mGyje9eVco"
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.AspNetCore.Hosting": "Warning",
                "Microsoft.AspNetCore.Routing": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "formatter": "NewRelic.LogEnrichers.Serilog.NewRelicFormatter, NewRelic.LogEnrichers.Serilog"
                }
            }
        ],
        "Properties": {
            "ApplicationName": "Zenko-API"
        },
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId",
            "WithEnvironmentName",
            "WithCorrelationId",
            "WithNewRelicLogsInContext"
        ]
    },
    "AllowedHosts": "*"
}